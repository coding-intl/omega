{
  "name": "@omen/examples",
  "description": "data driven javascript frontend library, based on JSX",
  "scripts": {
    "start": "npm run start:counter",
    "start:simple": "set EXAMPLE=simple&&webpack-dev-server --config webpack.develop.config.js --hot --inline",
    "start:counter": "set EXAMPLE=counter&&webpack-dev-server --config webpack.develop.config.js --hot --inline",
    "build": "npm run build:counter",
    "build:simple": "set EXAMPLE=simple&&set NODE_ENV=production&& webpack -p --config webpack.production.config.js",
    "build:counter": "set EXAMPLE=counter&&set NODE_ENV=production&& webpack -p --config webpack.production.config.js",
