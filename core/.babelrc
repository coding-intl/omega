{
  "presets": ["es2015", "stage-0", "stage-2"],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["transform-react-jsx", { "pragma": "omen.create" }]
  ],
  "env": {
    "publish": {
      "presets": [
        [
          "minify",
          {
            "mangle": false,
            "evaluate": false
          }
        ]
      ]
    }
  }
}
